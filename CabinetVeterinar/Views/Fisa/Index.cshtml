@model IEnumerable<CabinetVeterinar.Models.Programare>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            Nume prenume client
        </th>     
        <th>
            @Html.DisplayNameFor(model => model.Start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.End)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descriere)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Client.Nume)
            @Html.DisplayFor(modelItem => item.Client.Prenume)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_veterinar)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Start)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.End)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descriere)
        </td>
        <td>          
            @Html.ActionLink("Detalii ", "Detalii", new { id=item.Id_programare })          
        </td>
    </tr>
}

</table>
